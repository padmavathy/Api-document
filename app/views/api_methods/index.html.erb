<% @api_methods.each do |api_method| %>
  <tr>
    <td width="180px" style="font-size:16px;"><%= api_method.name %></td>
    <td width="50px" style="font-size:12px;"><%= link_to 'Show', api_method %></td>
    <td width="50px" style="font-size:12px;"><%= link_to 'Edit', edit_api_method_path(api_method) %></td>
    <td width="50px" style="font-size:12px;"><%= link_to 'Destroy', apimethod_destroy_path(api_method.id), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<p style="font-size:16px;">
